@model IEnumerable<clinicadental.Models.Examenextraoral>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Atm)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GanglioLinfatico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Otro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdRespiracionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Atm)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GanglioLinfatico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Otro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdRespiracionNavigation.IdRespiracion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdExamenExtraOral">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdExamenExtraOral">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdExamenExtraOral">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
